{% extends '_layout.jinja2' %}

{% block content %}
  <article>
    <h2>Projects</h2>

    <nav>
      <ul>
        <li><a href="#weasyprint">WeasyPrint</a></li>
        <li><a href="#cairosvg">CairoSVG</a></li>
        <li><a href="#tinycss2">tinycss2</a></li>
        <li><a href="#pyphen">Pyphen</a></li>
        <li><a href="#pydyf">pydyf</a></li>
